{
  "name": "chime-sdk-react-demo",
  "scripts": {
    "COMMENT": "Yes, this uses ./node_modules/.bin. That's because npm puts that binary path _at the end of PATH_. Sigh.",
    "fix-eval": "./node_modules/replace/bin/replace.js -r --include='*.js' 'eval.*\\(moduleName\\);' 'undefined;' ./node_modules/protobufjs/",
    "release": "npm run fix-eval && rm -rf dist/* && npm run package:attendee && npm run package:moderator && npm run package:talent && npm run package:broadcast",
    "package:attendee": "./node_modules/.bin/webpack --config src/apps/attendee/webpack.prod.js",
    "package:moderator": "./node_modules/.bin/webpack --config src/apps/moderator/webpack.prod.js",
    "package:talent": "./node_modules/.bin/webpack --config src/apps/talent/webpack.prod.js",
    "package:broadcast": "./node_modules/.bin/webpack --config src/apps/broadcast/webpack.prod.js",
    "start:attendee": "./node_modules/.bin/webpack-dev-server --hot --config src/apps/attendee/webpack.dev.js",
    "start:moderator": "./node_modules/.bin/webpack-dev-server --hot --config src/apps/moderator/webpack.dev.js",
    "start:talent": "./node_modules/.bin/webpack-dev-server --hot --config src/apps/talent/webpack.dev.js",
    "start:broadcast": "./node_modules/.bin/webpack-dev-server --hot --config src/apps/broadcast/webpack.dev.js"
  },
